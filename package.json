{
  "name": "hentai-downloader",
  "version": "1.0.0",
  "description": "A powerful CLI tool for downloading galleries with advanced features like scheduling, checkpointing, and batch processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:readme": "oclif readme --readme-path COMMANDS.md",
    "start": "node ./bin/run.js",
    "dev": "node ./bin/dev.js",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "downloader",
    "hitomi"
  ],
  "oclif": {
    "bin": "hentai-downloader",
    "dirname": "hentai-downloader",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {},
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>"
  },
  "bin": {
    "hentai-downloader": "./bin/run.js"
  },
  "author": "fa0311",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fa0311/hentai-downloader.git"
  },
  "homepage": "https://github.com/fa0311/hentai-downloader",
  "bugs": {
    "url": "https://github.com/fa0311/hentai-downloader/issues"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.36",
    "async-mutex": "^0.5.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "cli-truncate": "^5.1.1",
    "cron": "^4.4.0",
    "dotenv": "^17.2.3",
    "error-stack-parser": "^2.1.4",
    "fetch-socks": "^1.3.2",
    "jsonc-parser": "^3.3.1",
    "log-symbols": "^7.0.1",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "roaring": "^2.7.0",
    "shx": "^0.4.0",
    "string-width": "^8.1.0",
    "xmlbuilder2": "^4.0.3",
    "yazl": "^3.3.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^25.0.6",
    "@types/yazl": "^3.3.0",
    "oclif": "^4.22.65",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
